/* Hello World in Groovy */
pipelie {
         agent any 
                stages {
                     stage("1") {
                            steps {
                                 echo "hi, new file for workspace"    
                            }
                     }
                     stage("2") {
                           steps {
                             input{'do you want to proceed?'}
                            }
                     }
                     stage("3") {
                     when {
                            not {
                                   branch "master"
                                   }
                          }
                          steps {
                               echo "hello"
                               }
                      }
                    
                     stage ("4") {
                              stage ("unit test") {
                                                 steps {
                                                       echo 'Running the unit test'
                                                       }
                              stage ("integration test") {
                                                  steps {
                                                      echo "running integration test"
                                                  }
                              }
                         }                     
                    }
                     
                }
}
